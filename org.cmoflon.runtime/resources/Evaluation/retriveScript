EXPERIMENTS=(lmstGen lmstMan kTCMan lkTCMan kTCGen lkTCGen)
start=35571

for a  in "${EXPERIMENTS[@]}"
do
echo $a
for((i=start;i<start+10;i++))
	do
	mkdir ./$a/$i
	curl --user d.giessing:ortxfmy1 https://www.flocklab.ethz.ch/user/webdav/$i/results.tar.gz --output ./$a/$i/results.tar.gz
	tar -xzf ./$a/$i/results.tar.gz -C ./$a
	rm ./$a/$i/results.tar.gz
	done
	start=$(($start+10))
done
